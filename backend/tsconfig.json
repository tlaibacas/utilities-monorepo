{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",         
    "moduleResolution": "nodenext",   
    "rootDir": "src",             
    "outDir": "dist",             
    "strict": true,               
    "esModuleInterop": true,      
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
